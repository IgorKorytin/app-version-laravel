{
    "name": "avto-dev/app-version-laravel",
    "description": "Laravel applications versioning",
    "keywords": [
        "some",
        "keywords",
        "goes",
        "here"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "avto-dev",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "laravel/framework": ">=5.4.0 <5.7.0"
    },
    "require-dev": {
        "laravel/laravel": ">=5.4.0 <5.7.0",
        "phpunit/phpunit": "~5.7 || ^6.4 || ~7.0",
        "mockery/mockery": "0.9.* || ~1.0",
        "symfony/var-dumper": "~3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\AppVersion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\AppVersion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --no-coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\AppVersion\\AppVersionServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/app-version-laravel/issues",
        "source": "https://github.com/avto-dev/app-version-laravel"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
